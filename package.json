{
  "name": "xng-breadcrumb",
  "version": "1.0.2",
  "license": "MIT",
  "description": "A declarative and reactive breadcrumb for Angular 6 and beyond https://www.npmjs.com/package/xng-breadcrumb",
  "author": {
    "name": "Uday Vunnam",
    "email": "uday4vunnam@gmail.com"
  },
  "homepage": "https://github.com/udayvunnam/xng-breadcrumb",
  "bugs": "https://github.com/udayvunnam/xng-breadcrumb/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/udayvunnam/xng-breadcrumb.git"
  },
  "keywords": [
    "angular",
    "angular6",
    "angular2",
    "ng",
    "breadcrumb",
    "ngx"
  ],
  "scripts": {
    "watch:lib": "ng build xng-breadcrumb --watch",
    "watch:app": "ng serve xng-breadcrumb-app --open=true",
    "start": "npm-run-all --parallel copy:readme watch:* ",
    "build:lib": "ng build xng-breadcrumb",
    "build:app": "ng build xng-breadcrumb-app --prod  --progress=false",
    "build": "npm-run-all copy:* build:* link:lib",
    "copy:readme": "cp README.md projects/xng-breadcrumb/README.md",
    "copy:version": "replace '@VERSION' $npm_package_version ./projects/xng-breadcrumb/package.json",
    "link:lib": " npm link ./dist/xng-breadcrumb && npm link xng-breadcrumb",
    "release:lib": "npm publish ./dist/xng-breadcrumb",
    "deploy:app": "netlify deploy --prod --dir=dist/xng-breadcrumb-app",
    "test": "ng test --watch=false --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/cdk": "~7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "angular-in-memory-web-api": "^0.8.0",
    "core-js": "^2.5.4",
    "faker": "^4.1.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.11",
    "prismjs": "^1.24.0",
    "rxjs": "~6.3.3",
    "xng-breadcrumb": "latest",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/faker": "^4.1.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash": "^4.14.134",
    "@types/node": "~8.9.4",
    "@types/prismjs": "^1.16.0",
    "codelyzer": "~4.5.0",
    "husky": "^2.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "netlify-cli": "^2.11.15",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.0",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "replace": "^1.1.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
